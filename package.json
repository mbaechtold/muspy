    "autoprefixer": "^9.4.2",
    "babel-loader": "^8.0.4",
    "browserslist": "^4.3.5",
    "bulma": "^0.7.2",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-hot-loader": "^1.4.2",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "eslint-plugin-babel": "^5.3.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.10.0",
